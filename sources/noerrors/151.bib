@incollection{jelinek98,
   Author = {Jelinek, Eloise},
   Title = {Voice and transitivity as functional projections in {Y}aqui},
   BookTitle = {The projection  of arguments: {L}exical and compositional factors},
   Editor = {Butt, Miriam and Geuder, Wilhelm},
   Publisher = {CSLI},
   Address = {Stanford, CA},
   Pages = {177--206},
   Abstract = {The idea of argument structure as a distinct structural domain. p. 201: "On this kind of a compositional analysis, the VOICE and TRANSITIVE projections a) introduce arguments and assign theta roles to them; and b) provide the structural framework for case assignment. " "In a system where TRANSITIVE is a strong feature that is overt in the syntax, marked in an affix or "light verb" that is distinct from the lexical head of the cluase, it is possible for lexical items other than verbs to head finite sentences. ..Jelinek (1995a) argues that in Salish, where VOICE and TRANSITIVE are also overt projections, any openclass lexical item may head a finite clause" (202) 
Concerned also with thematic restrictions associated with particular argument positions (.e.g. implicit agents must be human in Yaqui; experiences cannot be direct objects in {Navajo}, etc; in Yaqui double accusative structures, the goal must be animate and affected; not the case with the ACC-DAT structures.},
      Year = {1998} }

@incollection{jelinek99,
   Author = {Jelinek, Eloise},
   Title = {Datives and argument hierarchies},
   BookTitle = {{Papers in honor of Ken Hale. Coyote Papers 12}},
   Publisher = {University of Arizona},
   Address = {Tucson},
   Abstract = {Extends ideas about pronominal argument languages, and the restrictions which hold between argument types and argument positions to datives. Surveys 5 languages and the way dative is realized in them. In the PA languages, there is no dative movement, bec. alternative realizations in dative clauses is fixed by argument type of goal and patient. Only in non PA languages like Yaqui and {English} is dative movement possible. "Higher-ranked goal arguments have structural case; and lower-ranked goals hae an inherent oblique case. Dative movement is excluded, because arguments that differ in rank also differ in structural position, and thus differ in their Case options. I argue that these hierarchies provide important evidence on the question  of Argument Type as a parametric feature, evidene that in Pronominal Argument langauges, pronouns have one set of structural positions and Case options, while nominals have another set."},
      Year = {1999} }

