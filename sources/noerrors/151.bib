@article{alexiadou98,
   Author = {Alexiadou, Artemis and Anagnostopoulou, Alena},
   Title = {{Parametrizing AGR: Word order, V-movement and EPP-checking}},
   Journal = {Natural Language \& Linguistic Theory},
   Volume = {16},
   Number = {3},
   Pages = {633--678},
   Note = {A theory which seeks to link VSO order with null subject pronouns. The EPP is universal, The EPP involves checking a +D feature on a non-nominal head (e.g. I or AGR). and is universal.  But it is parametrized and satisfiable in two ways: by XP movement or insertion ("merger"), or by Head movement. Lgs. like {English} satisfy it in the first way (either by inserting an expletive or moving the subject). Lgs. like {S}panish and Greek satisfy it in the second way, by raising V. V in these lgs. has a +D feature which permits it to check the +D feature in I (see paper for details of implementation). In these languages, preverbal "subject" position is an A' position (not in the same maximal projection as the finite verb), and they associate it with a CLLD construction (the element in that position is base-generated and binds a pronominal):a. the "subject" can be separated from V by adverbs (this assumes that adverbs do not attach to X', and requires an account of {English} cases like John probably as left)b. it is restricted to elements with wide scope (this follows if narrow scope is associated with VP-internal position and is preserved under movement; but there is no movement in these cases; c) in these lgs. an overt pronoun interpreted as bound variable cannot occur preverbally. This is interpreted as the consequence of a constraint which allows a pronoun in an A position (but not an Abar position) to be interpreted as a bound variable. The presence of DR effects (definiteness restriction) in existential contexts is a semantic effect (Keenan); the presence of DR effects in other contexts is a syntactic effect, tracable to the presence of an overt or covert expletive in subject position. Hence, the absence of a DR in VSO order points to there being no expletive.  There is a "core parameter" having to do with the presence of pronominal features in the verbal morphology. In lgs with this feature,  (e.g. Greek, {S}panish) verbal agreement morphology has the same status as pronouns in lgs. like {English}. "By hypothesis, EPP-checking is D-feature checking in a non-substantive category by a [nominal] lexical category" (p. 517) The parameter (p. 518) is conceived of as whether the EPP is checked by Move/Merge XP or by Move/Merge X.  This means that AGR is universally strong, and therefore universally present, in the terms of Chomsky 1995. A further independent parameter concerns the presence of [Spec, TP], a subject position outside {VP} but below the position for EPP checking. Lgs which make use of this position do so for Case reasons, and it is found in transitive expletive constructions (Icelandic) and is related to object shift (see Bobaljik and Jonas).},
      Year = {1998} }

@incollection{alexiadou02,
   Author = {Alexiadou, Artemis and Anagnostopoulou, Alena},
   Title = {Raising without infinitives and the role of agreement},
   BookTitle = {Dimensions of Movement: {F}rom features to remnants},
   Editor = {Alexiadou, Artemis and Anagnostopoulou, Elena and Barbiers, Sjef and Gaertner, Hans-Martin},
   Publisher = {John Benjamins},
   Address = {Amsterdam},
   Pages = {17--30},
   Note = {Argue (contra Chomsky 1998) for a disassociation between nominative case checking and agreement. The argument comes from Greek, where there is raising from finite clauses. They assume that A-movement is motivated by the need to check an EPP feature or Case feature. Since Raising proceeds from a clause with full agreement, they conclude that this agreement does not check Case. The need for Case drives Raising. The propose that Agreement is a PF reflex of formal feature checking either Case or EPP. They propose there are at least two types of Agreement-Case, Agreement-EPP relations: 1) Agreement is a reflex of N/Case-checking ({english}, portuguese); (ii) Agreement is the reflex of EPP checking (Greek, other Balkan languages). They argue further that Agreement is disassociated from Case when T lacks semantic tense. Their final proposal is that Agr is linked to EPP, while Tense is linked to Case. (So in embedded clause, there is agreement, which is associated with the EPP, but not with case assignment.) The tensed verb in the main clause is linked to Case.},
      Year = {2002} }

