@article{woolford91,
   Author = {Woolford, Ellen},
   Title = {{VP}-internal subjects in {VSO} and nonconfigurational languages},
   Journal = {Linguistic Inquiry},
   Volume = {22},
   Pages = {503--540},
   Note = {Argues that the subject in a number of VSO languages VSO order involves a {VP} internal subject. In some cases, it may occupy Spec, V. Given the existence of VP-internal subjects, there is no obstacle to seeing VSO languages as configurational. However, EW also suggests that subject may be sister to V, in a non-configurational structure (strongest evidence comes from blocked anaphora in {J}akaltek, *His father loves him (condition C violation under a flat structure). She also argues that there is configurational structure above the VP, in the IP projection.  Some discussion of the AF construction: following Woolford 1986, she regards blocked extraction of subjects in transitive clauses as a consequence of the 'peripheral gap constraint' (p. 520). Once VSO languages are removed from the class of nonconfigurational lgs, only a few strong candidates remain, according to EW, e.g. Papago and Walpiri. She presents some evidence that these too are configurational (cf. Legate).},
   Keywords = {nonconfigurationality, {M}ayan AF},
   Year = {1991} }

