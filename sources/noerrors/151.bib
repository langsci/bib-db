@article{cash,
   Author = {Cash, Philip Cash and Carnie, Andrew},
   Title = {{Nez Perce} Case: a note on interpretation and feature checking},
   Journal = {under review},
   Note = {The Erg-Acc is pattern involves vTRANS, which introduces the agent and assigns ergative case to it (a la Ura), and also checks accusative case. The accusative raises up and niches between the agent and little-v. The ‚àö‚àè ‚àö‚àè pattern of case marking involves intransitive clauses. The subject raises to Spec, T for nominative; the object is licensed within the {VP} by "pseudo-incorporation" or by "null oblique case". I.e. it's a sort of antipassive construction. They note information structure correlates with the two case patterns.},
   Abstract = {Abstract: We propose a refinement of Woolford's (1997) analysis of the multiple case
system of Nez Perce, providing an information structure grounding and formal
implementation for her MaxACC Generalization. In particular, we argue that in Nez
Perce, objects that are not prominent in information structure are caseless and the clauses
containing them are intransitive.},
