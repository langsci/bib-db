@article{alexopoulou2006,
     jstor_articletype = {research-article},
     title = {Resumption in Relative Clauses},
     author = {Alexopoulou, Theodora},
     journal = {Natural Language \& Linguistic Theory},
     jstor_issuetitle = {},
     volume = {24},
     number = {1},
     jstor_formatteddate = {Feb., 2006},
     pages = {57--111},
     url = {http://www.jstor.org/stable/27642821},
     ISSN = {0167806X},
     abstract = {The paper focuses on resumption in Greek relative clauses. It is argued that resumption arises on two occasions: (i) as a last resort strategy when identification of ?-features of non-arguments fails; (ii) in the absence of a syntactic feature on C trigerring AGREE/MOVE. In the first case obligatory resumption is restricted to non-argument positions and is sensitive to islands. This is the case of Greek null operator restrictive relatives. In particular, it is argued that identification requirements are grammaticised on Greek C (pu), which is endowed with ?-features. Failure of AGREE of the ?-features of pu leads to resumption in non-argument positions. In questions and operator restrictive relatives identification requirements are straightforwardly satisfied by the presence of an overt operator; resumption therefore is never obligatory in these structures. In the second case, where resumption is the consequence of absence of a syntactic relation (AGREE/MOVE) between C and the relativised site, resumption is obligatory in argument and non-argument positions (modulo the highest subject restriction) and insensitive to islands. Greek non-restrictive relatives and Semitic restrictive relatives exemplify this case.},
     year = {2006},
     publisher = {Springer},
     copyright = {Copyright ¬© 2006 Springer},
    }

