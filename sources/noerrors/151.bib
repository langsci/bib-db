@article{gregory01,
   Author = {Gregory, Michelle and Michaelis, Laura},
   Title = {Topicalization and Left Dislocation: {A} functional opposition revisited},
   Journal = {Journal of Pragmatics},
   Volume = {33},
   Pages = {1665--1706},
   Note = {comparison of functions of LD and Topicalization in {English}. Critique of Prince's earlier work. They conclude that both constructions are constrained by the 'poset' relation (partially ordered set relation) -- a relation between the preclausal {NP} and some previously evoked entity in the discourse model... (This is responsible for the contrastive element in both constructions). Contra Prince, they argue that LD has a topic promotion function that is lacking in topicalization. (This accounts for why an anaphoric element like 'that' can be topicalized, but not LD'd.) The methodology here involves computing an 'anaphoricity' index for dislocatee (its relation to previous discourse) and computing its perseverance in later discourse (a la Givon). Topic promotion associated with low anaphoricity and high perseverance. They do not discuss Prince's other claim which involves a kind of pairing of contrast in Topicalization: since the predicate is an open sentence in which the topicalized constituent is an argument, there is another point of contrast, denoted by the stressed constituent in the predicate. The result is two dimensions of contrast. They have a useful comparison of Gundel's Givenness Scale (which relates linguistic form to cognitive status) and Prince's Familiarity Scale, which involves notions like "textually evoked", "situationally evoked" (which are neutralized to "activated" in Gundel), and inferred (via relation to previuos discourse, via {NP} internal form (which neutralize to "uniquely identifiable" in Gundel).},
      Year = {2001} }

